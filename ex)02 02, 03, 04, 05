#include<stdio.h>
int main(){
	int a;
	int b;
	a=50
	b=100

	printf("a의 값은 %d 입니다 b의 값은 %d 입니다", a,b);
	return 0;
}

#include<stdio.h>
int main (){
	int a=30;
	printf("30의 제곱값은%d이다. \n", a*a);
	return 0;
}

#include <stdio.h>
int main (){
	char m,n;
	m='A';
	n=65;
	printf("m값 출력 : %c \n", m);
	printf("n 값 출력 :%c \n", n};
	printf("ASCII 코드 65 값 출력 : %c \n", 65);
	pirntf("ASCII 코드 65+1 값 출력 : %c \n", 65+1);
	printf("m+1값 출력 : %c \n", m+1);
	printf("n+1 값 출력 :%c \n", n+1};
	return 0;

}

/*증감 연산자 감소 연산자*/
#include <stdio.h>
{
	int i,j,k,m;
	i=10,j=10;
	k= i++;	/*후위증가 연산*/
	m=++j;	/*전위 증가 연산*/
	printf(" i = %d, k=%d\n",i,j);
	printf("j=%d, m=%d\n",j,m);
	return 0;
}
